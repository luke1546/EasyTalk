<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.easyback.study.model.mapper.StudyMapper">
  <select id="getList" resultType="String">
    select user_name
    from test
  </select>

  <select id="getWordList" parameterType="wordDto" resultType="wordDto">
    select *
    from words
  </select>
</mapper>
